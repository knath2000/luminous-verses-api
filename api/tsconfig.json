{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2020",
    "outDir": "../../dist",
    "rootDir": "../",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "baseUrl": ".",
    "paths": {
      "../prisma/generated/client": ["../prisma/generated/client"]
    }
  },
  "include": [
    "../**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}